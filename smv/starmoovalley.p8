-- ending

function fadeout(num)
	pal()
	for i=1,15 do
		local curr=i
		for _=1,num do
			curr=fade[curr]
			pal(i,curr)
		end
	end
end

function update_ending()
	endingframes+=1
	if endingframes>30 then
		cam.y=min(cam.y+1,550-screen/2+8)
	end
	camera(cam.x,cam.y)
end

function draw_ending()
	cls(1)
	palt(3,true)
	palt(0,false)
	scprint("happy birthday jade!",64,7,5)
	
	for i=1,#cows do
		local x=40*((i%3)+1)-24
		local y=200+flr((i-1)/3)*40
		local tx=ctext(cows[i].name,x,x+cows[i].w)
		
		spr(cows[i].sprnum,x,y,2,2)
		sprint(cows[i].name,tx,y+cows[i].h+4,7,5)
	end
	
	spr(100,56,550,2,2)
end
