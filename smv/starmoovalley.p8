function _init()
	screen=128
	mapsize=2*screen -- i'm not sure if i ever use this
	threshold=20
	displaying=false -- displaying any message?
	showgift=false

	cam={x=0,y=0}
	p1=createplayer()
	cows=generatecows()
	cowlist=generatecowlist()
	gift=creategift(mapsize/2-8,mapsize/2-8)

	fade={0,1,1,2,1,13,6,4,4,9,3,13,1,13,14}
	endingtimer=nil
	endingdelay=15
	endingframes=0
	
	_upd=update_title
	_drw=draw_title
end

function _update()
	_upd()
end

function _draw()
	_drw()
end
