-- main behavior

function update_main()
	-- check if gift should be drawn
	if not showgift then
		if #cowlist==0 then
			showgift=true
			music(4)
		end
	end
	
	if endingtimer~=nil and t()-endingtimer>endingdelay then
		_upd=update_ending
		_drw=draw_ending
		cam.x=0
		cam.y=0
		camera(cam.x,cam.y)
		pal()
		return
	end

	p1:update()

	for c in all(cows) do
		c:update()
	end

	if showgift then
		gift:update()
	end

	cam.x=mid(0,p1.x-screen/2+p1.w/2,mapsize-screen)
	cam.y=mid(0,p1.y-screen/2+p1.h/2,mapsize-screen)
	camera(cam.x,cam.y)
end
