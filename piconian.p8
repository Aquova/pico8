pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
-- piconian
-- by aquova

function _init()
	t=0 --general purpose timer
	screen=128
	mapsize=4*screen
 bulletspeed=3
	-- states:
	-- 0 - title screen
	-- 1 - main
	-- 2 - paused
	-- 3 - game over
	state=0
	ship={x=64,y=64,direc=2,sprt=1,flp=false}
	cam={x=0,y=0}
	bullets={}
	lives=3
	setenemies()
end

function setenemies()
 for i=0,2 do
 	for j=4,6 do
 		mset(j,i,(i*16)+j)
 	end
 end
end

function drawmap()
	local topx=flr(cam.x/128)*128
	local topy=flr(cam.y/128)*128
	local colx=topx/8
	local coly=topy/8
	if colx < 0 then
		colx=48
	end
	if coly < 0 then
		coly=48
	end

	map(colx,coly,topx,topy,16,16)
	map((colx+16)%64,coly,topx+screen,topy,16,16)
	map(colx,(coly+16)%64,topx,topy+screen,16,16)
	map((colx+16)%64,(coly+16)%64,topx+screen,topy+screen,16,16)
end

function newbullet()
	local b={}
	--offset by 4 to center bullet
	b.x=ship.x+4
	b.y=ship.y+4
	b.orien=ship.sprt
	b.direc=0
	b.draw=function(this)
		circfill(b.x,b.y,1,9)
	end
	b.update=function(this)
		if b.orien==1 then
			if b.direc==0 then
				b.y-=bulletspeed
			else
				b.y+=bulletspeed
			end
			if b.y < cam.y or b.y > (cam.y+screen) then
				del(bullets,b)
			end
		else
			if b.direc==0 then
				b.x-=bulletspeed
			else
				b.x+=bulletspeed
			end
			if b.x < cam.x or b.x > (cam.x+screen) then
				del(bullets,b)
			end
		end
	end
	b.collide=function(this)
		local sprx=flr((b.x%mapsize)/8)
		local spry=flr((b.y%mapsize)/8)
		local sprite=mget(sprx,spry)
		if fget(sprite,0) then
			del(bullets,b)
			mset(sprx,spry,sprite+6)
		elseif fget(sprite,1) then
			del(bullets,b)
			deleteenemy(sprx-1,spry-1)
		elseif fget(sprite,2) then
			del(bullets,b)
		end
	end
	return b
end

function deleteenemy(x,y)
	--x and y are for top left sprite
	for i=0,2 do
 	for j=0,2 do
 		mset(x+i,y+j,0)
 	end
 end	
end

function debug()
 print("",cam.x,cam.y,12)
	print("x: "..ship.x)
	print("y: "..ship.y)
	print("cam.x: "..cam.x.." cam.y "..cam.y)
end

function centertext(t)
 return (screen/2)-#t*2
end

function titlescreen()
	cls()
	print("piconian",centertext("piconian"),20,11)
	print("by aquova",centertext("by aquova"),30,11)
	spr(1,60,50)
	print("press ❎ to start",centertext("press ❎ to start"),70,12)
end

function _update()
	if state==0 then
		if btnp(5) then
			state=1
		end
	elseif state==2 then
		if btnp(4) then
			state=1
		end
	elseif state==3 then
		if btnp(4) or btnp(5) then
			_init()
		end
	else
  if btn(0) then
   ship.direc=0
  elseif btn(1) then
   ship.direc=1
  elseif btn(2) then
   ship.direc=2
  elseif btn(3) then
   ship.direc=3
  end

  if ship.direc==0 then
  	ship.x-=1
  	ship.sprt=2
  	ship.flp=true
  elseif ship.direc==1 then
  	ship.x+=1
  	ship.sprt=2
  	ship.flp=false
  elseif ship.direc==2 then
  	ship.y-=1
  	ship.sprt=1
  	ship.flp=false
  else
  	ship.y+=1
  	ship.sprt=1
  	ship.flp=true
  end

  if btnp(4) then
  	state=2
  end

  if btnp(5) then
  	if count(bullets) == 0 then
   	add(bullets,newbullet())
   	b=newbullet()
   	b.direc=1
   	add(bullets,b)
   	sfx(0)
  	end
  end

  if ship.x < 0 then
  	ship.x=4*screen
  elseif ship.x > 4*screen then
  	ship.x=0
  end

  if ship.y < 0 then
  	ship.y=4*screen
  elseif ship.y > 4*screen then
  	ship.y=0
  end

		local sprite=mget(flr(ship.x/8),flr(ship.y/8))
		if fget(sprite,0) or fget(sprite,1) or fget(sprite,2) then
			lives-=1
			if lives < 1 then
				cls()
				state=3
			end
			ship.x=64
			ship.y=64
		end 

  for bullet in all(bullets) do
   bullet:update()
			bullet:collide()
  end
  cam.x=ship.x-(screen/2)
  cam.y=ship.y-(screen/2)
  camera(cam.x,cam.y)
 end
end

function _draw()
 if state==0 then
  titlescreen()
 elseif state==2 then
 	print("paused",cam.x+57,cam.y+55,8)
 elseif state==3 then
		print("game over",centertext("game over"),64,9)
 else
  cls()
  drawmap()
  spr(ship.sprt, ship.x, ship.y, 1, 1, ship.flp, ship.flp)

  for bullet in all(bullets) do
   bullet:draw()
  end
		local lcounter="lives: "..lives
		print(lcounter,cam.x+centertext(lcounter),cam.y,12)
  --debug()
 end
end

