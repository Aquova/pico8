-- player functions

function move_player()
	local startx=p1.x
	p1.dx=0
	
	-- horizontal movement
	if btn(‚¨ÖÔ∏è) then
		p1.dx=-p1.vx
		p1.flp=false
	end
	if btn(‚û°Ô∏è) then
		p1.dx=p1.vx
		p1.flp=true
	end
	
	p1.x+=p1.dx
	-- horizontal screen transition
	if (p1.x-cam.x)>screen then
		cam.x+=screen
		camera(cam.x,cam.y)
	elseif (p1.x-cam.x)<0 then
		cam.x-=screen
		camera(cam.x,cam.y)
	end
	
	-- check for wall
	local xoffset=0
	if p1.dx>0 then xoffset=7 end
	
	-- check flag of bottom center of player
	-- this may need to be tweaked
	local v=mget((p1.x+xoffset)/8,(p1.y+7)/8)
	if fget(v,0) then
		p1.x=startx
	end
	
	-- vertical movement
	if btn(‚ùé) and p1.isgrounded then
		p1.dy=p1.vy
	end
	
	-- check for floor
	v=mget((p1.x+4)/8,(p1.y+8)/8)
	p1.isgrounded=false
	
	if p1.dy>=0 then
		if fget(v,0) then
			p1.y=flr(p1.y/8)*8
			p1.dy=0
			p1.isgrounded=true
		elseif fget(v,2) then
			-- spring behavior
			p1.dy=1.5*p1.vy
		end
	end
	
	p1.dy+=world.grav
	p1.y+=p1.dy

	-- check for ceiling
	v=mget((p1.x+4)/8,p1.y/8)
	if p1.dy<=0 then
		if fget(v,0) then
			p1.dy=0
			p1.y=flr((p1.y+8)/8)*8
		end
	end
	
	-- vertical screen transition
	if (p1.y-cam.y)>screen then
		cam.y+=screen
		camera(cam.x,cam.y)
	elseif (p1.y-cam.y)<0 then
		cam.y-=screen
		camera(cam.x,cam.y)
	end
	
	-- set animation frame
	if p1.x!=startx then
		p1.anim=(p1.anim+1)%4 -- mod 4 so sprite changes every other frame
	else
		p1.anim=0
	end
end

function check_for_item()
	local v=mget(flr(p1.x/8),flr(p1.y/8))
	if fget(v,1) then
		if v==16 then
			world.r=true
		elseif v==17 then
			world.y=true
		else
			world.b=true
		end
		mset(flr(p1.x/8),flr(p1.y/8),0)	
		change_monochrome()
		return
	end
	
	v=mget(ceil(p1.x/8),flr(p1.y/8))
	if fget(v,1) then
		if v==16 then
			world.r=true
		elseif v==17 then
			world.y=true
		else
			world.b=true
		end
		mset(ceil(p1.x/8),flr(p1.y/8),0)	
		change_monochrome()
		return
	end
	
	v=mget(flr(p1.x/8),ceil(p1.y/8))
	if fget(v,1) then
		if v==16 then
			world.r=true
		elseif v==17 then
			world.y=true
		else
			world.b=true
		end
		mset(flr(p1.x/8),ceil(p1.y/8),0)	
		change_monochrome()
		return
	end
	
	v=mget(ceil(p1.x/8),ceil(p1.y/8))
	if fget(v,1) then
		if v==16 then
			world.r=true
		elseif v==17 then
			world.y=true
		else
			world.b=true
		end
		mset(ceil(p1.x/8),ceil(p1.y/8),0)	
		change_monochrome()
		return
	end	
end
