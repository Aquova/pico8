-- spectra
-- by aquova

function _init()
	screen=128
	debug=false
	first_title=true
	title_particles={}
	
	p1={
		x=568,
		y=336,
		flp=false,
		anim=0, -- animation frame
		size=8,
		dx=0,
		dy=0,
		vx=2, -- movement speeds
		vy=-5,
		isgrounded=false
	}
	
	cam={x=512,y=256}
	
	world={
		grav=0.4,
		r=false,
		y=false,
		b=false
	}
	
	bkgd_color=5
	monochrome={[0]=0,0,5,5,5,5,6,7,6,6,6,6,6,5,7,6}

	_upd=update_title
	_drw=draw_title
end

function _update()
	_upd()
end

function _draw()
	_drw()
end
